{
  "name": "media-processor-api",
  "version": "1.0.0",
  "description": "Video and audio processing API with CPU-intensive tasks",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "dev:start": "node scripts/dev-start.js",
    "test": "jest",
    "test:files": "node scripts/test-file-management.js",
    "test:cpu": "node scripts/quick-cpu-test.js",
    "test:stress": "node scripts/cpu-stress-test-advanced.js",
    "setup-env": "node scripts/setup-env.js",
    "docker:build": "docker build -t media-processor-api .",
    "docker:run": "docker run -p 3000:3000 media-processor-api"
  },
  "keywords": [
    "video",
    "audio",
    "processing",
    "api",
    "cpu-intensive"
  ],
  "author": "Student",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.887.0",
    "@aws-sdk/client-dynamodb": "^3.887.0",
    "@aws-sdk/client-elasticache": "^3.887.0",
    "@aws-sdk/client-rds": "^3.887.0",
    "@aws-sdk/client-s3": "^3.887.0",
    "@aws-sdk/client-secrets-manager": "^3.887.0",
    "@aws-sdk/client-ssm": "^3.887.0",
    "@aws-sdk/s3-request-presigner": "^3.887.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "pg-pool": "^3.6.1",
    "sharp": "^0.33.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "watch": [
    "app.js",
    "scripts/",
    "public/",
    "database/"
  ],
  "ignore": [
    "node_modules/",
    "uploads/",
    "processed/",
    "database/*.db",
    "*.log",
    ".git/",
    "logs/"
  ]
}

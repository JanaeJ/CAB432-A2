{
  "watch": [
    "app.js",
    "scripts/",
    "public/"
  ],
  "ignore": [
    "node_modules/",
    "uploads/",
    "processed/",
    "database/*.db",
    "*.log",
    ".git/"
  ],
  "ext": "js,json,html,css",
  "env": {
    "NODE_ENV": "development",
    "DEBUG": "true"
  },
  "delay": "1000",
  "verbose": true
}
